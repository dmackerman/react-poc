{
	"presets": [
		"es2015",
		"stage-1",
		"react"
	],
	"plugins": [
		"transform-decorators-legacy",
		"transform-class-properties",
		"transform-object-rest-spread",
		"transform-regenerator"
	],
	"env": {
		"development": {
			"plugins": [
				["react-transform", {
					"transforms": [{
						"transform": "react-transform-hmr",
						"imports": ["react"],
						"locals": ["module"]
					}]
				}]
			]
		}
	}
}
